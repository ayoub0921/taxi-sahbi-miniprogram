.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 40px;
  background-color: var(--color-primary);
  min-height: 100vh;
  box-sizing: border-box;
  position: relative;
}


.headLogo{

  position: absolute;
  left: 0;
  top: 0;
}



.logo {
  width: 80px;
  height: 80px;
  margin-top: 40px;
}

.thanks-btns {
  margin-top: 60px;
  display: flex;
  flex-direction: column;
  gap: 10px;
}
